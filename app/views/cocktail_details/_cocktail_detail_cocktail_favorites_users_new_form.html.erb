<%= form_with(model: @cocktail_favorites_user, local: true) do |f| %>

  <%= f.hidden_field :user_id, value: current_user.id %>

  <%= f.hidden_field :cocktail_id, value: @cocktail_detail.id %>

  <%= f.submit "Create cocktail favorites user", class: "btn btn-block btn-outline-secondary" %>
<% end %>
