<%= form_with(model: @user_bar_cart, local: true) do |f| %>

  <%= f.hidden_field :user_id, value: @user.id %>

  <div class="form-group">
    <%= f.label :spirit_brand_id %>

    <%= f.collection_select :spirit_brand_id, SpiritsBrand.all, :id, :to_s, {}, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :quantity %>

    <%= f.text_field :quantity, class: "form-control" %>
  </div>

  <%= f.submit "Create user bar cart", class: "btn btn-block btn-outline-secondary" %>
<% end %>
